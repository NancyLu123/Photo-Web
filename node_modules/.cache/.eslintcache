      </div>\r\n          )}\r\n        </div>\r\n        {data.length !== 0 && (\r\n          <div className=\"morePicture\">\r\n            <button onClick={morepicture}>Load More</button>\r\n          </div>\r\n        )}\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Homepage;\r\n","/Users/nancy/Desktop/PhotoWeb/Photo-Web/src/components/CarouselPicture.js",[],"/Users/nancy/Desktop/PhotoWeb/Photo-Web/src/components/Picture.js",["54","55","56"],"import React from \"react\";\r\nimport \"./Picture.sass\";\r\n\r\nconst Picture = ({ data }) => {\r\n  return (\r\n    <div className=\"picture\">\r\n      <p>Author：{data.photographer}</p>\r\n      <div className=\"imageContainer\">\r\n        <a target=\"_blank\" href={data.src.large}>\r\n          <img src={data.src.large} alt=\"\" />\r\n        </a>\r\n      </div>\r\n      <p>\r\n        {/*新增頁面*/}\r\n        <a target=\"_blank\" href={data.src.large}></a>\r\n      </p>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Picture;\r\n","/Users/nancy/Desktop/PhotoWeb/Photo-Web/src/api/photoApi.js",[],"/Users/nancy/Desktop/PhotoWeb/Photo-Web/src/api/Api.js",[],"/Users/nancy/Desktop/PhotoWeb/Photo-Web/src/api/api.js",[],{"ruleId":"57","severity":1,"message":"58","line":2,"column":8,"nodeType":"59","messageId":"60","endLine":2,"endColumn":11},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":43,"column":6,"nodeType":"67","endLine":43,"endColumn":21,"suggestions":"68"},{"ruleId":"69","severity":1,"message":"70","line":9,"column":9,"nodeType":"71","messageId":"72","endLine":9,"endColumn":50,"fix":"73"},{"ruleId":"69","severity":1,"message":"70","line":15,"column":9,"nodeType":"71","messageId":"72","endLine":15,"endColumn":50,"fix":"74"},{"ruleId":"75","severity":1,"message":"76","line":15,"column":9,"nodeType":"71","endLine":15,"endColumn":50},"no-unused-vars","'Nav' is defined but never used.","Identifier","unusedVar","no-native-reassign",[